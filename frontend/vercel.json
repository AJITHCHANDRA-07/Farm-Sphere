{
  "version": 2,
  "builds": [
    {
      "src": "farmsphere-backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "farmsphere-visions-main/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/farmsphere-backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/farmsphere-visions-main/$1"
    }
  ]
}

{
  "name": "farmsphere-backend",
  "version": "1.0.0",
  "description": "FarmSphere Backend API with Ollama Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-crops": "node generate-crops-with-ollama.js",
    "generate-short": "node -e \"require('./generate-crops-with-ollama.js').generateSpecificCategory('short', 100)\"",
    "generate-medium": "node -e \"require('./generate-crops-with-ollama.js').generateSpecificCategory('medium', 150)\"",
    "generate-long": "node -e \"require('./generate-crops-with-ollama.js').generateSpecificCategory('long', 200)\"",
    "extract-ollama": "node extract-existing-ollama-data.js",
    "migrate-extracted": "node migrate-extracted-crops.js",
    "full-ollama-migration": "node extract-existing-ollama-data.js && node migrate-extracted-crops.js",
    "extract-all-models": "node extract-all-ollama-models.js",
    "replace-all-data": "node extract-all-ollama-models.js",
    "extract-from-api": "node extract-from-ollama-api.js",
    "complete-replacement": "node extract-from-ollama-api.js"
  },
  "keywords": [
    "agriculture",
    "crops",
    "ollama",
    "ai",
    "supabase",
    "api"
  ],
  "author": "FarmSphere Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.11"
  }
}
